pipeline {
  alphabet{
    floor f #907558; 
	  entrance e #00FF00;
	  spawn s #C8A2C8;
	  wall * #8080800;
  }
	options {
		seed: 12;
		size: 25x25;
		tiletype: f; 
	}
	module environment {
		rules {
		  addSpawn:
		    f -> s;
		}
		
		recipe {
		    level_entrance = addSpawn;
		}
		;
	}
	module the_place {
		rules {
		  addWalls:
			fffffffffff,
			fffffffffff,
			fffffffffff,
			fffffffffff,
			fffffffffff,
			fffffffffff,
			fffffffffff,
			fffffffffff
			->
			***********,
			*fffffffff*,
			*fffffffff*,
			*fffffffff*,
			*fffffffff*,
			*fffffffff*,
			*fffffffff*,
			***********;
			addEntrance: 
			*,
			f 
			->  
			e,
			f;
		}
		recipe {
		  	room = addWalls; 
			place_entrance = addEntrance[in room];
		    path = CreatePath(place_entrance,level_entrance);
		}
		;
	}
	module vault {
		rules {
		  addWalls:
			ffff,
			ffff,
			ffff,
			ffff
			->
			****,
			*ff*,
			*ff*,
			****;
			addEntrance: 
			*f -> ef;
		}
		recipe {
		    vault_room = addWalls[in the_place]; 
			vault_entrance = addEntrance[in vault_room];
			path1 = CreatePath(place_entrance,vault_entrance);
		}
        on exit constraint inner_path: Intact(path1);
	}
	on exit constraint a: vault in the_place;
    on exit constraint b: Intact(path);
}