pipeline a {
	seed: 12;
	
	alphabet {
		floor f #010101 
		wall  * #111111
		entrance  e #123456
		exit  x #000000
		pillar | #qwqqwq
		water ~ #123123
	}	
	
	options {
		size: 6x6;
		tiletype: f;
	}
	
	module first_module {
		rules {
			addEntrance: 
			*,
			f 
			-> 
			e,
			f;
			addExit: 
			*f 
			-> 
			xf;
		}
	}
	
	
}

